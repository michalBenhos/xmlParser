<?xml version="1.0"?>
<config testName="Config">
	
	<setupComponents>
	
		<installPlugins>
			
			<plugin>
				<name>UpdateDatabaseTraceAreas</name>
				<enabled>1</enabled>
			</plugin>
			
			<pluginPost>
				<name>UpdateDatabaseTraceAreasForIduServer</name>
				<enabled>1</enabled>
			</pluginPost>
			 
		</installPlugins>
		
		<component>
			<name>DatAdvantage</name>
			<enabled>1</enabled>
			<type>iduModule</type>

			<params>

					<customKey>
						<name>-idu-datasource</name>
						<value>CI15-DV1\R2</value>/value>
					</customKey>
					
					<customKey>
						<name>-idu-server</name>
						<value>CI15-DV1</value>
					</customKey>
					
					<customKey>
						<name>-dp-datasource</name>
						<value>CI15-DV1\R2</value>
					</customKey>
					
					<customKey>
						<name>-dp-dbuser</name>
						<value>sa-lab</value>
					</customKey>
					
					<customKey>
						<name>-dp-dbpassword</name>
						<value>p@ssword1</value>
					</customKey>
					
				</params>
			
		</component>

		<component>
			<name>DAReports</name>
			<enabled>0</enabled>
			<type>iduModule</type>
			<params></params>
		</component>

		<component>
			<name>Classification</name>
			<enabled>0</enabled>
			<type>iduModule</type>
			<params></params>
			<installNamePattern>*Data Classification Engine*</installNamePattern>
		</component>

		<component>
			<name>SecureMigration</name>
			<enabled>0</enabled>
			<type>iduModule</type>
			<params></params>
		</component>

		<component>
			<name>DataPrivilege</name>
			<enabled>1</enabled>
			<type>iduModule</type>
			<params></params>
            <installNamePattern>*DataPrivilege Web*</installNamePattern>
		</component>

		<component>
			<name>DPReports</name>
			<enabled>1</enabled>
			<type>iduModule</type>
			<params></params>
		</component>

		<component>
			<name>SynchronizationService</name>
			<enabled>1</enabled>
			<type>iduModule</type>
			<params></params>
		</component>

		<component>
			<name>GUI</name>
			<enabled>1</enabled>
			<type>external</type>
			<params>GUI\DatAdvantage.msi</params>
			<installNamePattern>*DatAdvantage UI*</installNamePattern>
		</component>

		<component>
			<name>ManagementConsole</name>
			<enabled>1</enabled>
			<type>external</type>
			<params>Management Console\Varonis.ManagementConsole.Setup.msi</params>
			<installNamePattern>*Management Console*</installNamePattern>
		</component>

		<component>
			<name>BulkInsert</name>
			<enabled>1</enabled>
			<type>external</type>
			<params>Tools\Bulk Upload\DataPrivilege.BulkInsert.Installer.msi</params>
			<installNamePattern>*DataPrivilege Bulk Upload Utility*</installNamePattern>
		</component>

	</setupComponents>
	
</config>